config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "GameScript",
"language": "GDScript",
"path": "res://Scripts/GameScript.gd"
}, {
"base": "Reference",
"class": "WAT",
"language": "GDScript",
"path": "res://addons/WAT/namespace.gd"
}, {
"base": "Node",
"class": "WATTest",
"language": "GDScript",
"path": "res://addons/WAT/test/test.gd"
} ]
_global_script_class_icons={
"GameScript": "",
"WAT": "",
"WATTest": ""
}

[WAT]

Test_Directory="res://UnitTests/"
Results_Directory="res://"
Test_Metadata_Directory="res://"
Tags=PoolStringArray(  )
Cache_Tests=true
Window_Size=Vector2( 1280, 720 )
Minimize_Window_When_Running_Tests=false
Port=6008
Display=8

[application]

config/name="Bloom Game"
run/main_scene="res://Scenes/Game.tscn"
config/icon="res://icon.png"

[autoload]

GlobalVariables="*res://Scripts/GlobalVariables.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/WAT/plugin.cfg" )

